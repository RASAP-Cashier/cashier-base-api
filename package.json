{
  "name": "api-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "node ./node_modules/pm2/bin/pm2-runtime start ecosystem.develop.config.js",
    "lint": "eslint \"app/**/*.{js,ts}\" --fix",
    "prettier": "prettier --write --print-width 120  --trailing-comma all \"**/*.+(ts|js)\"",
    "commit-hook": "npm run jsdoc && npm run prettier && npm run lint && git add . && npm run test",
    "start": "node ./node_modules/pm2/bin/pm2-runtime start ecosystem.config.js",
    "test": "NODE_ENV=test jest --runInBand --forceExit --coverage",
    "knex": "knex --knexfile app/knexConfig.ts",
    "jsdoc": "jsdoc -c jsdoc.json -r -d docs"
  },
  "pre-commit": [
    "commit-hook"
  ],
